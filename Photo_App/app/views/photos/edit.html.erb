<h1>Edit photo</h1>

<div class="well">
<%= form_for @photo, html: { multipart: true } do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label "Photo" %>
<%# f.text_field :attachment %>
<%= image_tag(@photo.attachment_url, size: "350") %>
<%= f.submit "Save", class: "btn btn-primary" %>
<% end %>

</div>